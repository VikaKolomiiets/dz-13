<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite first" verbose="2">
    <test name="Parameter Test one">
        <parameter name="name" value="Wong" />
        <classes>
            <class name="getsettests.PersonGetterTests">
                <methods>
                    <include name="testGetFirstNamePositive" />
                    <include name="testGetLastNamePositive" />
                    <include name="testGetBirthLastNamePositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test two">
        <parameter name="yyyy" value="1976"/>
        <parameter name="mm" value="10"/>
        <parameter name="dd" value="11"/>
        <classes>
            <class name="getsettests.PersonGetterTests">
                <methods>
                    <include name="testGetDateOfBirthPositive" />
                    <include name="testPersonGetDateOfDeathPositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test three">
        <parameter name="name1" value="Ann"/>
        <parameter name="name2" value="Lezard"/>
        <classes>
            <class name="getsettests.PersonGetterTests">
                <methods>
                    <include name="testPersonGetPartnerPositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test four">
        <parameter name="status" value="IS_DIVORCED"/>
        <classes>
            <class name="getsettests.PersonGetterTests">
                <methods>
                    <include name="testPersonGetStatusPositive" />
                </methods>
            </class>
        </classes>
    </test>
</suite>
